pragma solidity ^0.8.17;
contract minmax{
int8 public min8 = type(int8).min;
int8 public max8=type(int8).max;
int16 public min16 = type(int16).min;
int16 public max16=type(int16).max;
uint8 public umin8 = type(uint8).min;
uint8 public umax8=type(uint8).max;
uint16 public umin16 = type(uint16).min;
uint16 public umax16=type(uint16).max;
int24 public min24 = type(int24).min;
int24 public max24=type(int24).max;
int32 public min32 = type(int32).min;
int32 public max32=type(int32).max;
uint24 public umin24 = type(uint24).min;
uint24 public umax24=type(uint24).max;
uint32 public umin32 = type(uint32).min;
uint32 public umax32=type(uint32).max;


}